{
	"browser": {
		"./lib/node/main.js": "./lib/browser/main.js"
	},
	"description": "Lightweight request library. Promise based, with proxy support.",
	"devDependencies": {
		"@types/node": "16.x",
		"ava": "^5.3.1",
		"http-proxy-agent": "^5.0.0",
		"https-proxy-agent": "^5.0.1",
		"proxy": "^2.1.1",
		"rimraf": "^5.0.1",
		"ts-loader": "^9.4.4",
		"typescript": "^5.2.2",
		"typescript-esbuild": "0.4.5",
		"vscode-jsonrpc": "^5.0.0",
		"vscode-nls": "^5.2.0",
		"webpack": "^5.88.2",
		"webpack-cli": "^5.1.4"
	},
	"main": "./lib/node/main.js",
	"name": "request-light",
	"scripts": {
